{
  "name": "tna/tna-base-child-template",
  "description": "This is a custom wordpress theme to use as a child site for tna-base as a dependency",
  "type": "wordpress-theme",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Punal Chotrani",
      "email": "punal.chotrani@nationalarchives.gsi.gov.uk"
    }
  ],
  "repositories": [
    {
      "name": "nationalarchives/tna-base-child-template",
      "type":"package",
      "package": {
        "source": {
          "url": "https://github.com/nationalarchives/tna-base-child-template.git",
          "type": "git"
        }
      }
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "nationalarchives/tna-base-child-template": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*",
    "squizlabs/php_codesniffer": "1.*",
    "wordpress/coding-standards": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "minimum-stability": "dev"
}